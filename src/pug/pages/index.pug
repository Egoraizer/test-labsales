    .advantages.index__advantages
      .container.advantages__wrapper
        h2.advantages__title О комплексе
        .swiper#index-grid-cards
          ul.swiper-wrapper.advantages__cards
              each card in cardsType2
                  li.swiper-slide.card.card_type-2.advantages__card
                    +cardType2(card)
          .swiper-pagination
    .infrastructure.index__infrastructure
      .infrastructure__wrapper
        h2.container.infrastructure__title Наша инфраструктура
        .infrastructure__map
          #index-infrastructure-map(style="width: 100%; height: 100%;")
          .swiper.infrastructure__map-description#index-infrastructure-cards
            ul.swiper-wrapper.infrastructure__cards
              each card in cardsType3
                li.swiper-slide.card.card_type-3.infrastructure__card
                  +cardType3(card)
            .swiper-scrollbar
    .residential.index__residential-description
